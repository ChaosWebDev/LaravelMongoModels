{
    "name": "chaoswd/laravel-mongo-models",
    "description": "A lightweight Laravel package that adds an Artisan command for generating MongoDB Eloquent models using the official mongodb/laravel driver.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mongodb",
        "artisan",
        "model",
        "generator",
        "eloquent",
        "chaoswd"
    ],
    "authors": [
        {
            "name": "Jordan Gerber",
            "email": "jordangerber@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "mongodb/laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Chaoswd\\Mongo\\Models\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chaoswd\\Mongo\\Models\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chaoswd\\Mongo\\Models\\Providers\\MongoModelsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/ChaosWebDev/LaravelMongoModels/issues",
        "source": "https://github.com/ChaosWebDev/LaravelMongoModels"
    }
}